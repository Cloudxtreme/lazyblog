{{ define "new" }}{{ template "header" . }}
<main>
  <h1>New Post</h1>
  <form action="/new" method="POST">
    <input type="text" name="id" placeholder="Title">
    <br>
    <input type="text" name="body" placeholder="Body">
    <br>
    <input type="submit" value="Submit">
  </form>
</main>{{ template "footer" }}{{ end }}
